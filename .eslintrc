{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/react",
    "react-app"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "no-nested-ternary": [
      "off"
    ],
    "react/no-array-index-key": [
      false
    ],
    "import/prefer-default-export": [
      true
    ],
    "import/no-unresolved": [
      false
    ],
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [
          ".ts",
          ".tsx"
        ]
      }
    ],
    "react/prop-types": 0,
    "no-underscore-dangle": 0,
    "import/imports-first": [
      "error",
      "absolute-first"
    ],
    "import/newline-after-import": "error"
  },
  "globals": {
    "window": true,
    "document": true,
    "localStorage": true,
    "FormData": true,
    "FileReader": true,
    "Blob": true,
    "navigator": true
  },
  "parser": "babel-eslint"
}
